		<array>
			<string>com.homemakers.merchant.source_language_download.task</string>
			<string>com.homemakers.merchant.target_language_download.task</string>
			<string>com.homemakers.merchant.new_language_download.task</string>
			<string>com.homemakers.merchant.english_language_download.task</string>
			<string>com.homemakers.merchant.english_language_download.task</string>
		</array>
